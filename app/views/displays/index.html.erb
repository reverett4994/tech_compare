

<p id="notice"><%= notice %></p>

<h1>Listing Displays</h1>


<table>
  <thead>
    <tr>
      <th>Price</th>
      <th>Name</th>
      <th>Resolution</th>
      <th>Brand</th>
      <th>Aspect ratio</th>
      <th>Size</th>
      <th>Ports</th>
      <th>Response time</th>
      <th>Refresh rate</th>
      <th>Speakers</th>
      <th>Dimensions</th>
      <th>Panel</th>
      <th>Release</th>
      <th>Stand misc</th>
      <th>Gpu sync</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @displays.each do |display| %>
      <tr>
        <td><%= display.Price %></td>
        <td><%= display.name %></td>
        <td><%= display.resolution %></td>
        <td><%= display.brand %></td>
        <td><%= display.aspect_ratio %></td>
        <td><%= display.size %></td>
        <td><%= display.ports %></td>
        <td><%= display.response_time %></td>
        <td><%= display.refresh_rate %></td>
        <td><%= display.speakers %></td>
        <td><%= display.dimensions %></td>
        <td><%= display.panel %></td>
        <td><%= display.release %></td>
        <td><%= display.stand_misc %></td>
        <td><%= display.gpu_sync %></td>
        <td><%= link_to 'Show', display %></td>
        <td><%= link_to 'Edit', edit_display_path(display) %></td>
        <td><%= link_to 'Destroy', display, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Display', new_display_path %>
